{
  "name": "tke-console-web",
  "version": "1.0.0",
  "description": "Console web app for TKEStack",
  "main": "index.js",
  "engines": {
    "node": "v14.17.4"
  },
  "scripts": {
    "preinstall": "rm -rf node_modules",
    "prestart-tke": "npm  install",
    "start-tke": "webpack server  --config ./webpack/webpack.dev.js --progress --color --env version=tke",
    "prestart-project": "npm install",
    "start-project": "webpack server  --config ./webpack/webpack.dev.js --progress --color --env version=project",
    "build-tke": "webpack --config ./webpack/webpack.prod.js --progress --color --env version=tke",
    "build-project": "webpack --config ./webpack/webpack.prod.js --progress --color --env version=tke --env version=project",
    "prebuild": "rm -rf build && npm install",
    "build": "npm run build-tke && npm run build-project",
    "postbuild": "gulp",
    "prepare": "cd ../../ && husky install ./web/console/.husky",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "./**/*.{ts,tsx,js,jsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "license": "Apache-2.0",
